<h1>Contact Us</h1>
<h2>Who do you want to contact?</h2>

<form action="/contact" method="get">
<label for="member">Search here -></label>
  <input type="text" name="member" id="member" placeholder="Jane Doe" value="<%= params[:member]%>">

  <input type="submit">
</form>

<ol>
  <% @members.each do |member| %>
  <li><%= member.capitalize %></li>
  <% end %>
</ol>

<%= link_to "Home", root_path %>
